<tryton>
    <data>

        <!-- company.company -->
        <record model="ir.ui.view" id="company_view_form">
            <field name="model">company.company</field>
            <field name="inherit" ref="company.company_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath
                        expr="/form/notebook/page[@id=&quot;company&quot;]/field[@name=&quot;parent&quot;]"
                        position="after">
                        <separator string="Domains" colspan="4" id="domains"/>
                        <label name="idomain_alert_expire"/>
                        <field name="idomain_alert_expire"/>
                    </xpath>
                </data>
                ]]>
            </field>
        </record>

    </data>
</tryton>
